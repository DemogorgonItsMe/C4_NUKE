-- epic ui stroke giver
 
for i,v in pairs(MainGui) do
	if v:IsA("TextBox") then
	else
		if v:IsA("Frame") or v:IsA("TextButton") then
			local uistroke = Instance.new("UIStroke")
			if v:IsA("TextButton") then
				uistroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
			else
				uistroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
			end
			uistroke.Color = Color3.fromRGB(0,0,0)
			uistroke.Thickness = 2
			uistroke.Transparency = 0
			uistroke.Parent = v
			if v:IsA("Frame") then
				coroutine.wrap(function()
					while wait() do
						uistroke.Color = Color3.fromHSV(tick() % 3/3,1,1)
					end
				end)()
			end
		elseif v:IsA("TextLabel") then
			local uistroke = Instance.new("UIStroke")
			uistroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
			uistroke.LineJoinMode = Enum.LineJoinMode.Round
			uistroke.Color = Color3.fromRGB(0,0,0)
			uistroke.Thickness = 1
			uistroke.Transparency = 0
			uistroke.Parent = v
		end
	end
end
 
 
 
local nondupe = false
local dupe = false
local ToDupe = 0
local WeaponId = 0
local Players = game:GetService("Players")
local plr = Players.LocalPlayer
local UIS = game:GetService("UserInputService")
 
local function GetClosest() -- devforum getclosest :sob:
    local Character = plr.Character
    local HumanoidRootPart = Character and Character:FindFirstChild("HumanoidRootPart")
    if not (Character or HumanoidRootPart) then return end
 
    local TargetDistance = math.huge
    local Target
 
    for i,v in ipairs(Players:GetPlayers()) do
        if v ~= plr and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
            local TargetHRP = v.Character.HumanoidRootPart
            local mag = (HumanoidRootPart.Position - TargetHRP.Position).magnitude
            if mag < TargetDistance then
                TargetDistance = mag
                Target = v
            end
        end
    end
 
    return Target
end
 
UIS.InputBegan:Connect(function(K,J)
	if J then return end
	K = K.KeyCode
	if K == Enum.KeyCode[Keybind1:upper()] then
	    if nondupe == false then
		    nondupe = true
		else
		    nondupe = false
	    end
	    print(nondupe)
		if nondupe then
			MainGui.Text5.Text = "true"
			MainGui.Text5.TextColor3 = Color3.fromRGB(0, 255, 0)
		else
			MainGui.Text5.Text = "false"
			MainGui.Text5.TextColor3 = Color3.fromRGB(255, 0, 0)
		end
	end
	if K == Enum.KeyCode[Keybind2:upper()] then
		if dupe == false then
		    dupe = true
		else
		    dupe = false
		end
		print(dupe)
		if dupe then
			MainGui.Text6.Text = "true"
			MainGui.Text6.TextColor3 = Color3.fromRGB(0, 255, 0)
		else
			MainGui.Text6.Text = "false"
			MainGui.Text6.TextColor3 = Color3.fromRGB(255, 0, 0)
		end
	end
end)
 
MainGui.Button1.MouseButton1Down:Connect(function()
	if ToDupe == 0 then
		ToDupe = 10
	end
	if WeaponId == 0 then
		WeaponId = 2
	end
	local dupe1 = {}
 
	for i=1,ToDupe do table.insert(dupe1,tonumber(WeaponId)) end
 
	settings().Network.IncomingReplicationLag = math.huge
	task.wait()
	for i,v in pairs(dupe1) do
   		local h = v
 
   		local args = {
       		[1] = h,
       		[2] = "0:0:0:0"
		}
		game:GetService("ReplicatedStorage").Item:FireServer(unpack(args))
	end
	task.wait(0.5)
	settings().Network.IncomingReplicationLag = 0
	task.wait(#dupe1/19)
	for i,v in pairs(plr.Backpack:GetChildren()) do
		if i == 4 then
			MainGui.Button1.Text = "Dupe failed."
			wait(2)
			MainGui.Button1.Text = "Dupe weapon"
		end
   		if v.Name == "Katana" or v.Name == "Fire bomb" or v.Name == "Grenade" or v.Name == "Bear trap" then
       		v:Destroy()
   		end
	end
end)
 
MainGui.Box3:GetPropertyChangedSignal("Text"):Connect(function()
	if MainGui.Box3.Text ~= 0 then
		WeaponId = tonumber(MainGui.Box3.Text)
	end
end)
 
MainGui.Box4:GetPropertyChangedSignal("Text"):Connect(function()
	if MainGui.Box4.Text ~= 0 then
		ToDupe = tonumber(MainGui.Box4.Text)
	end
end)
 
-- Non-dupe
 
coroutine.wrap(function()
	while wait() do
	    if nondupe then
		    pcall(function()
			    for i,v in pairs(plr.Character:GetDescendants()) do
            	    if v:IsA("RemoteEvent") and v.Name == "hit" then
                	    v.Parent.swing:FireServer()
               		    v:FireServer(GetClosest().Character.Humanoid)
            	    end
			    end
		    end)
	    end
	    if Keybind1:lower() == Keybind2:lower() then -- putting it here because no wait and i dont wanna make another while wait() do
			Keybind1 = "Z"
			Keybind2 = "X"
			MainGui.Box1.Text = "Z"
			MainGui.Box2.Text = "X"
	    end
	end
end)()
 
-- Dupe
 
coroutine.wrap(function()
	while wait(0.09) do
	    if dupe then
    	    pcall(function()
        	    for i,v in pairs(plr.Backpack:GetChildren()) do
           		    if v:IsA'Tool' and v.Name == "C4" or v.Name == "Grenade" or v.Name == "Fire bomb" or v.Name == "Beartrap" then
            	    else
                	    if v:IsA'Tool' then
                		    plr.Character.Humanoid:EquipTool(v)
                		    v.swing:FireServer()
                		    v.hit:FireServer(GetClosest().Character.Humanoid)
                	    end
            	    end
        	    end
    	    end)
	    end
	end
end)()
 
 
local key = game:GetService("UserInputService")
 
key.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.Z then
		local tool = "C4"
 
for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
if v:IsA("Tool") and v.Name == tool then
v.Parent = game:GetService("Players").LocalPlayer.Character
if v.Name == tool then
v:Activate()
end
end
end
 
	end
end)
